		"onCommand:js-debug-companion.launchAndAttach",
		"onCommand:js-debug-companion.kill",
		"onCommand:js-debug-companion.launch"
	],
	"dependencies": {
		"duplexer3": "^1.0.0",
		"execa": "^5.1.1",
		"split2": "^4.2.0",
		"ws": "^8.13.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/duplexer3": "^0.1.1",
		"@types/node": "^17.0.21",
		"@types/split2": "^3.2.1",
		"@types/ws": "^8.5.2",
		"esbuild": "^0.18.11",
		"rimraf": "^3.0.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "resources/logo.png",
	"api": "none",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": false
	},
	"extensionKind": [
		"ui"
	]
